<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="methods" data-provider-thread-count="5">
    <listeners>
        <listener class-name="org.cognito.orchestrationlayer.TestngListener"></listener>
    </listeners>
    <test verbose="1" preserve-order="true"
          name="Regression Suite">
        <classes>
            <class name="org.cognito.orchestrationlayer.runners.RegressionTestRunner" />
            <parameter name="executionMode" value="LOCAL"></parameter>
            <parameter name="browser" value="CHROME_HEADLESS"></parameter>
        </classes>
    </test>
</suite>